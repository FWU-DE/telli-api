{
  "name": "@dgpt/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development dotenv -e ../../.env -- tsx src/index.ts",
    "clean": "find . -type d \\( -name node_modules -o -name dist -o -name .next \\) -not -path \"./.vercel/*\" -prune -exec rm -rf {} \\;",
    "start": "NODE_ENV=production tsx src/index.ts",
    "build": "node build.js",
    "lint": "tsc --noEmit && eslint --max-warnings 0",
    "types": "tsc --noEmit",
    "test": "vitest run --coverage",
    "test:watch": "dotenv -e .env.test -- vitest run --watch",
    "test:coverage": "dotenv -e .env.test -- vitest run --coverage"
  },
  "packageManager": "pnpm@9.12.3",
  "dependencies": {
    "@dgpt/db": "workspace:*",
    "@dgpt/llm-model": "workspace:*",
    "@dgpt/utils": "workspace:*",
    "@fastify/cors": "11.1.0",
    "@fastify/multipart": "9.2.1",
    "@fastify/swagger": "9.5.2",
    "@fastify/swagger-ui": "5.2.3",
    "@opentelemetry/instrumentation-pg": "0.59.0",
    "@sentry/node": "10.19.0",
    "@sentry/profiling-node": "10.19.0",
    "fastify": "5.6.1",
    "openai": "5.20.1",
    "openaiv4": "npm:openai@4.80.1",
    "tiktoken": "1.0.22",
    "tsx": "4.20.6",
    "zod": "3.25.1"
  },
  "devDependencies": {
    "@dgpt/eslint-config": "workspace:*",
    "@dgpt/typescript-config": "workspace:*",
    "@types/node": "22.13.14",
    "@vitest/coverage-v8": "3.2.4",
    "dotenv": "17.2.3",
    "dotenv-cli": "10.0.0",
    "eslint": "8.57.0",
    "typescript": "5.7.2",
    "vitest": "3.2.4"
  }
}
