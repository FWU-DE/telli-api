{
  "name": "@dgpt/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development dotenv -e ../../.env -- tsx src/index.ts",
    "clean": "find . -type d \\( -name node_modules -o -name dist -o -name .next \\) -not -path \"./.vercel/*\" -prune -exec rm -rf {} \\;",
    "start": "NODE_ENV=production tsx src/index.ts",
    "build": "node build.js",
    "lint": "tsc --noEmit && eslint --max-warnings 0",
    "types": "tsc --noEmit",
    "test": "vitest run --coverage",
    "test:watch": "dotenv -e .env.test -- vitest run --watch",
    "test:coverage": "dotenv -e .env.test -- vitest run --coverage"
  },
  "dependencies": {
    "@dgpt/db": "workspace:*",
    "@dgpt/llm-model": "workspace:*",
    "@dgpt/utils": "workspace:*",
    "@fastify/cors": "11.1.0",
    "@fastify/multipart": "9.3.0",
    "@fastify/otel": "0.14.0",
    "@fastify/swagger": "9.6.1",
    "@fastify/swagger-ui": "5.2.3",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.67.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.208.0",
    "@opentelemetry/resources": "2.2.0",
    "@opentelemetry/sdk-metrics": "2.2.0",
    "@opentelemetry/sdk-node": "0.208.0",
    "@opentelemetry/sdk-trace-node": "2.2.0",
    "@opentelemetry/semantic-conventions": "1.38.0",
    "@sentry/node": "10.27.0",
    "@sentry/opentelemetry": "10.26.0",
    "@sentry/profiling-node": "10.26.0",
    "fastify": "5.6.2",
    "google-auth-library": "10.5.0",
    "openai": "6.9.1",
    "openaiv4": "npm:openai@4.104.0",
    "tiktoken": "1.0.22",
    "tsx": "4.20.6",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@dgpt/eslint-config": "workspace:*",
    "@dgpt/typescript-config": "workspace:*",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "4.0.13",
    "dotenv": "17.2.3",
    "dotenv-cli": "11.0.0",
    "eslint": "9.39.1",
    "typescript": "5.9.3",
    "vitest": "4.0.13"
  }
}
