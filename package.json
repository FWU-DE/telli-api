{
  "name": "dgpt",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "start:api": "turbo run start -F @dgpt/api",
    "clean": "find . -type d \\( -name node_modules -o -name dist -o -name .next \\) -not -path \"./.vercel/*\" -prune -exec rm -rf {} \\;",
    "dev": "turbo run dev",
    "dev:api": "dotenv -e .env -- turbo run dev -F @dgpt/api",
    "db:migrate": "dotenv -e .env -- turbo run db:migrate -F @dgpt/db",
    "db:seed": "dotenv -e .env -- turbo run db:seed -F @dgpt/db",
    "db:seed:from-stage": "dotenv -e .env -- turbo run db:seed:from-stage -F @dgpt/db",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "types": "turbo run types",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "dotenv-cli": "11.0.0",
    "prettier": "3.8.1",
    "turbo": "2.8.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "pnpm@10.24.0",
  "pnpm": {
    "overrides": {
      "glob@<11.1.0": ">=11.1.0"
    }
  }
}
